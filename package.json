{
  "$path": "package.json",
  "name": "cond8-docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./src/styles.css -o public/dist/styles.css",
    "cf-typegen": "wrangler types",
    "deploy": "wrangler deploy",
    "dev": "node scripts/dev.mjs",
    "dev:css": "tailwindcss -i ./public/styles.css -o ./public/output.css --watch",
    "dev:js": "esbuild scripts/*.ts --bundle --outdir=public/scripts --target=es2020 --watch",
    "start": "wrangler dev"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@tailwindcss/cli": "^4.1.1",
    "@tailwindcss/postcss": "^4.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dedent": "^1.5.3",
    "hono": "^4.7.5",
    "jest-diff": "^29.7.0",
    "mdast-util-from-markdown": "^2.0.2",
    "miniflare": "^4.20250321.2",
    "preact": "^10.26.4",
    "preact-render-to-string": "^6.5.13",
    "remark-parse": "^11.0.0",
    "tailwind-merge": "^3.1.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250404.0",
    "@swc/core": "^1.11.16",
    "@types/unist": "^3.0.3",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "tailwindcss": "^4.1.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.2",
    "wrangler": "^4.7.1"
  }
}
